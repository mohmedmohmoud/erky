<odoo>
    <data>
        <!-- erky contract sequences -->
        <record id="sequence_erky_contract" model="ir.sequence">
            <field name="name">Erky contract sequence</field>
            <field name="code">erky.contract</field>
            <field name="prefix">ERKY/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

         <!-- erky export form sequences -->
        <record id="sequence_erky_export_form" model="ir.sequence">
            <field name="name">Erky Export Form sequence</field>
            <field name="code">erky.export.form</field>
            <field name="prefix">EXFO/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

        <record id="sequence_erky_shipment" model="ir.sequence">
            <field name="name">Erky Shipment sequence</field>
            <field name="code">erky.vehicle.shipment</field>
            <field name="prefix">SHP/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

        <record id="sequence_erky_internal_contract" model="ir.sequence">
            <field name="name">Erky Internal Contract</field>
            <field name="code">erky.internal.contract</field>
            <field name="prefix">%(range_year)s/CV/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>