<odoo>
    <data>

        <report id="action_print_cheque_report"
            model="printing.cheque.wiz"
            string="Print Cheque"
            report_type="qweb-pdf"
            name="account_cheque.printing_cheque_template"
            file="account_cheque.printing_cheque_template"/>

        <template id="printing_cheque_template">
             <t t-call="web.basic_layout">
                <div class="page" t-att-style="'width:' + str(data['template_data']['cheque_w'])+ 'px;' + 'height:' + str(data['template_data']['cheque_h'])+ 'px;border:1px solid black;'">
                    <div class="oe_structure"/>
                    <!-- Holder-->
                    <div t-att-style="'width:' + str(data['template_data']['account_holder_w'])+ 'px;'
                     + 'height:' + str(data['template_data']['account_holder_h'])+ 'px;border:1px solid black;'
                     + 'margin-left:' + str(data['template_data']['account_holder_x'])+ 'px;'
                     + 'margin-top:' + str(data['template_data']['account_holder_y'])+ 'px;'">
                        <t t-esc="data['wiz_data']['account_holder_id'][1]"/>
                    </div>
                </div>
                </t>
        </template>
    </data>
</odoo>